<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.james.wawamachine">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/wawamap"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="AIzaSyDHnWFJYSeAJ5u__tMN9NY-U62PBDlk2YM" />

        <activity
            android:name=".SetDataActivity"
            android:label="@string/title_activity_set_data"
            android:theme="@style/MyMaterialTheme">
        </activity>
        <activity
            android:name=".MemberActivity"
            android:label="@string/title_activity_member"
            android:theme="@style/MyMaterialTheme">
        </activity>
        <activity
            android:name=".ListMyActivity"
            android:label="@string/title_activity_list_my"
            android:theme="@style/MyMaterialTheme">
        </activity>
    </application>

</manifest>